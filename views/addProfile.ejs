  <div class="aa-title" style="color: #6CB4EE;" ><h1 >Add Your Profile</h1></div>
  <div class="containerr">
      <div class="form-wrapper">
  <form action="/addProfile" method="POST" enctype="multipart/form-data" onsubmit="return validateForm()">
      <label for="name">Name:</label>
      <input type="text" name="name" class="form-control" value="" required><br>

      <label for="age">Age:</label>
      <input type="text" name="age" class="form-control" value="" required><br>

      <label for="specialization">Specialization:</label>
      <input type="text" name="specialization" class="form-control" value="" required><br>

      <label for="lookingFor">Looking For:</label>
      <select name="lookingFor" required>
          <option value="Roommate" >Roommate</option>
          <option value="Apartment" >Apartment</option>
          <option value="Roommate+Apartment" >Roommate+Apartment</option>
      </select><br>

      <label for="budget">Budget:</label>
      <input type="text" name="budget" class="form-control" value="" required><br>

      <label for="location">Location:</label>
      <input type="text" name="location" class="form-control" value="" required><br>

      <label for="phoneNumber">Phone Number:</label>
      <input type="text" name="phoneNumber" id="phoneNumberInput" maxlength="10" class="form-control" value="" required><br>

      <div class="form-group">
          <label for="image">Profile Image</label>
          <input type="file" name="image"   class="form-control" id="image">
        </div>

        <button type="submit" class="btn btn-primary profile-button">Save Changes</button>
      </form>
  </div>
